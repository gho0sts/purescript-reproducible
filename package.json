{
  "name": "purescript-quickstart",
  "version": "0.1.0",
  "scripts": {
    "start": "pulp --watch browserify --optimise --to js/index.js",
    "dist": "mkdir -p dist/js && uglifyjs js/*.js -m -o dist/js/index.js && cp index.html dist/"
  },
  "dependencies": {
    "pulp": "^12.4.2",
    "purescript": "^0.12.5",
    "purescript-hedwig": "^0.1.0",
    "uglify": "^0.1.5"
  }
}
